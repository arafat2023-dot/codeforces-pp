def find_equation(a, b, c):
  
  if a + b == c:
    return '+'
  elif a - b == c:
    return '-'
  else:
    return None


def main():
 
  t = int(input())
  for _ in range(t):
    a, b, c = map(int, input().split())
    result = find_equation(a, b, c)
    print(result)


if __name__ == '__main__':
  main()
